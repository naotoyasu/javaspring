<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.S001.mapper.UserMapper">
    <select id="findById" resultType="com.example.S001.entity.User">
        SELECT
            id,
            name
         FROM
            sampledb.user
         WHERE
            id = #{id}
    </select>
    <select id="findAll" resultType="com.example.S001.entity.User">
        SELECT
            id,
            name
         FROM
            sampledb.user
    </select>
</mapper>